<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <extension
         point="org.eclipse.ui.commands">
      <command
            name="%command.name"
            id="com.jaspersoft.studio.community.command.issueSubmit">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="com.jaspersoft.studio.community.command.issueSubmit"
            class="com.jaspersoft.studio.community.IssueSubmissionHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:help">
            <command
                  commandId="com.jaspersoft.studio.community.command.issueSubmit"
            	  mnemonic="%command.mnemonic"
                  id="com.jaspersoft.studio.community.menu.issueSubmit">
            </command>
                        
      </menuContribution>
   </extension>
</plugin>
