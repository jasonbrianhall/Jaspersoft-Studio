<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.jaspersoft.studio.rcp.intro.Application">
         </run>
      </application>
   </extension>
   
   <extension
         point="org.eclipse.ui.intro">
      <intro
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            id="com.jaspersoft.studio.rcp.intro">
      </intro>
      <introProductBinding
            introId="com.jaspersoft.studio.rcp.intro"
            productId="com.jaspersoft.studio.rcp.product">
      </introProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            introId="com.jaspersoft.studio.rcp.intro"
            content="introContent.xml"
            id="com.jaspersoft.studio.rcp.configId">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx">
            </implementation>
         </presentation>
      </config>
   </extension>
 <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.jaspersoft.studio.rcp.application"
            name="Jaspersoft Studio">
         <property
               name="windowImages"
               value="icons/jss_16.png,icons/jss_icon_32.png,icons/jss_icon_48.png,icons/jss_icon_64.png,icons/jss_icon_128.png">
         </property>
         <property
               name="aboutText"
               value="Jaspersoft Studio - Visual Designer for JasperReports. &#x0A;Copyright (C) 2010 - 2011 Jaspersoft Corporation. All rights reserved. &#x0A;&#x0A;For more information, please visit &#x0A;http://jasperforge.org/projects/jaspersoftstudio  &#x0A;&#x0A;Special thanks to all people who support this software.">
         </property>
         <property
               name="aboutImage"
               value="icons/studio_logo_about.png">
         </property>
         <property
               name="startupForegroundColor"
               value="0061A2">
         </property>
         <property
               name="startupProgressRect"
               value="0,260,362,5">
         </property>
         <property
               name="startupMessageRect"
               value="4,240,358,18">
         </property>
         <property
               name="appName"
               value="Jaspersoft Studio">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
      </product>
   </extension> 
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <action
            name="cheatshdatasource"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenCheatSheetAction&amp;csid=com.jaspersoft.studio.cheatsheets.datasource">
      </action>
      <action
            name="cheatshnewrep"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenCheatSheetAction&amp;csid=com.jaspersoft.studio.cheatsheets.designreport">
      </action>
      <action
            name="cheatshpreview"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenCheatSheetAction&amp;csid=com.jaspersoft.studio.cheatsheets.runreport">
      </action>
      <action
            name="jaspersoftsite"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;csid=jasperforge.org">
      </action>
      <action
            name="jaspersoftsitestudio"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;csid=jasperforge.org/projects/jaspersoftstudio">
      </action>
      <action
            name="jaspersoftsitestudiobugtracker"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;csid=jasperforge.org/projects/jaspersoftstudio/tracker/">
      </action>
      <action
            name="jaspersoftsitestudioforum"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;csid=jasperforge.org/projects/jaspersoftstudio/forum/">
      </action>
      <action
            name="jaspersoftfacebook"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;csid=www.facebook.com/pages/Jaspersoft-Corporation/78981369547">
      </action>
      <action
            name="banner"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;csid=jasperforge.org/projects/jasperserver">
      </action>
      <action
            name="tutorials"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;csid=jasperforge.org%2fwebsite%2fireportwebsite%2fJSS+Website%2ftutorials.html%3fheader%3dproject%26target%3djaspersoftstudio">
      </action>
      
      <!-- twitter.com/#!/JaspersoftS -->
      <action
            name="jaspersofttwitter"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;enc=base64&amp;csid=dHdpdHRlci5jb20vIyEvSmFzcGVyc29mdFMK">
      </action>
      <action
            name="jaspersoftserver"
            replaces="runAction?pluginId=com.jaspersoft.studio.rcp&amp;class=com.jaspersoft.studio.rcp.intro.action.OpenInBrowserAction&amp;csid=jasperforge.org/projects/jasperserver">
      </action>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.jaspersoft.studio.rcp.Startup"></startup>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.jaspersoft.studio.rcp.application"
            name="Jaspersoft Studio">
         <property
               name="windowImages"
               value="icons/jss_16.png,icons/jss_icon_32.png,icons/jss_icon_48.png,icons/jss_icon_64.png,icons/jss_icon_128.png">
         </property>
         <property
               name="aboutText"
               value="Jaspersoft Studio - Visual Designer for JasperReports. &#x0A;Copyright (C) 2010 - 2011 Jaspersoft Corporation. All rights reserved. &#x0A;&#x0A;For more information, please visit &#x0A;http://jasperforge.org/projects/jaspersoftstudio  &#x0A;&#x0A;Special thanks to all people who support this software.">
         </property>
         <property
               name="aboutImage"
               value="icons/studio_logo_about.png">
         </property>
         <property
               name="startupForegroundColor"
               value="0061A2">
         </property>
         <property
               name="startupProgressRect"
               value="0,260,362,5">
         </property>
         <property
               name="startupMessageRect"
               value="4,240,358,18">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="appName"
               value="Jaspersoft Studio">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="com.jaspersoft.studio.rcp.actionSet"
            label="Cheat Sheets"
            visible="true">
        <action label="Cheat Sheets" class="org.eclipse.ui.cheatsheets.CheatSheetExtensionFactory:helpMenuAction" menubarPath="help/group.tutorials" id="org.eclipse.ui.cheatsheets.actions.CheatSheetHelpMenuAction">
     </action>
      </actionSet>
   </extension>
   <extension point="org.eclipse.ui.menus">
   	<menuContribution
    	allPopups="false"
        locationURI="toolbar:org.eclipse.ui.main.toolbar">
		<toolbar
        	id="com.jaspersoft.studio.rcp.jssToolbar"
            label="Jaspersoft Studio Toolbar">
		    <command
		          commandId="org.eclipse.ui.newWizard"
		          style="push">
		       <parameter
		             name="newWizardId"
		             value="com.jaspersoft.studio.wizards.ReportNewWizard">
		       </parameter>
	 	        <visibleWhen checkEnabled="false">
	                <with variable="activeWorkbenchWindow.activePerspective">
	                  <equals value="com.jaspersoft.studio.ReportDesignPerspective"/>
	            	</with>
			    </visibleWhen>
		    </command>
		    <command
		          commandId="org.eclipse.ui.newWizard"
		          style="push">
		       <parameter
		             name="newWizardId"
		             value="com.jaspersoft.studio.data.wizard.NewFileDataAdapterWizard">
		       </parameter>
	 	        <visibleWhen checkEnabled="false">
	                <with variable="activeWorkbenchWindow.activePerspective">
	                  <equals value="com.jaspersoft.studio.ReportDesignPerspective"/>
	            	</with>
			    </visibleWhen>		       
		    </command>
		   	<command
		          commandId="org.eclipse.ui.newWizard"
		          style="push">
		       <parameter
		             name="newWizardId"
		             value="com.jaspersoft.studio.wizards.StyleTemplateNewWizard">
		       </parameter>
	 	        <visibleWhen checkEnabled="false">
	                <with variable="activeWorkbenchWindow.activePerspective">
	                  <equals value="com.jaspersoft.studio.ReportDesignPerspective"/>
	            	</with>
			    </visibleWhen>	
		    </command>
         </toolbar>
	</menuContribution>
	</extension>
</plugin>
