<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="com.jaspersoft.studio.rcp.intro.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="com.jaspersoft.studio.rcp.application"
            name="Jaspersoft Studio">
         <property
               name="appName"
               value="Jaspersoft Studio">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.intro">
      <intro
            class="org.eclipse.ui.intro.config.CustomizableIntroPart"
            id="com.jaspersoft.studio.rcp.intro">
      </intro>
      <introProductBinding
            introId="com.jaspersoft.studio.rcp.intro"
            productId="com.jaspersoft.studio.rcp.product">
      </introProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.config">
      <config
            introId="com.jaspersoft.studio.rcp.intro"
            content="introContent.xml"
            id="com.jaspersoft.studio.rcp.configId">
         <presentation
               home-page-id="root">
            <implementation
                  kind="html"
                  os="win32,linux,macosx">
            </implementation>
         </presentation>
      </config>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.jaspersoft.studio.rcp.Startup"></startup>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.jaspersoft.studio.ReportDesignPerspective"
            icon="icons/jr_icon.png"
            id="com.jaspersoft.studio.ReportDesignPerspective"
            name="Report Design">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="com.jaspersoft.studio.ReportDesignPerspective">
         <newWizardShortcut
               id="com.jaspersoft.studio.wizards.ReportNewWizard">
         </newWizardShortcut>
         <newWizardShortcut
               id="com.jaspersoft.studio.wizards.StyleTemplateNewWizard">
         </newWizardShortcut>
      </perspectiveExtension>
   </extension>

</plugin>
