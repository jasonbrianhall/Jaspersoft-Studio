                    "mandatory": false,
                    "defaultValue": "9",
                    "readOnly": false,
                    "type": "float"
                },
                {
                    "name": "isBold",
                    "label": "Bold",
                    "description": "Check if the font should be bold",
                    "mandatory": false,
                    "defaultValue": "false",
                    "readOnly": false,
                    "type": "checkbox_selector"
                },
                {
                    "name": "isItalic",
                    "label": "Italic",
                    "description": "Check if the font should be italic",
                    "mandatory": false,
                    "defaultValue": "false",
                    "readOnly": false,
                    "type": "checkbox_selector"
                }
            ]
        }
    ]
}