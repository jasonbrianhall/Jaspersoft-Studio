{
    "label": "Category Marker",
    "description": "A chart customizer for Category Charts",
    "customizerClass": "net.sf.jasperreports.chartcustomizers.CategoryMarkerCustomizer",
    "supportedPlot": ["1","2","3","7","8","11","12","20","21"],
    "sections": [
        {
            "name": "Customizer configuration",
            "expandable": false,
            "properties": [
                {
                    "name": "key",
                    "label": "Key",
                    "description": "The key",
                    "mandatory": true,
                    "defaultValue": "",
                    "readOnly": false,
                    "type": "text"
                },
                {
                    "name": "type",
                    "label": "Type",
                    "description": "The type",
                    "mandatory": false,
                    "defaultValue": "",
                    "readOnly": false,
                    "type": "selectable_combo",
                    "comboOptions": [["",""], ["line","Line"]]
                },
                {
                    "name": "label",
                    "label": "Label",
                    "description": "The content of the label",
                    "mandatory": false,
                    "defaultValue": "",
                    "readOnly": false,
                    "type": "text"
                },
                {
                    "name": "label-position",
                    "label": "Label Position",
                    "description": "The position of the label",
                    "mandatory": false,
                    "defaultValue": "top-left",
                    "readOnly": false,
                    "type": "selectable_combo",
                    "comboOptions": [["top","Top"],["top-left","Top Left"],["top-right", "Top Right"],["bottom","Bottom"],["bottom-left","Bottom Left"],["bottom-right","Bottom Right"],["center","Center"],["left","Left"],["right","Right"]]
                },
                {
                    "name": "label-text-position",
                    "label": "Label Text Anchor",
                    "description": "Sets the label text anchor and sends a MarkerChangeEvent to all registered listeners",
                    "mandatory": false,
                    "defaultValue": "center",
                    "readOnly": false,
                    "type": "selectable_combo",
                    "comboOptions": [["baseline_center","Baseline"],["baseline_left","Baseline Left"],["baseline_right", "Baseline Right"],["bottom_center","Bottom"],["bottom_left","Bottom Left"],["bottom_right","Bottom Right"],["center","Center"],["center_left","Center Left"],["center_right","Center Right"],["half_ascent_center","Half Ascent"],["half_ascent_left","Half Ascent Left"],["half_ascent_right", "Half Ascent Right"],["top_center","Top"],["top_left","Top Left"],["top_right", "Top Right"]]
                },
                {
                    "name": "label-position-offset-top",
                    "label": "Label Position Offset Top",
                    "description": "The top offset of the label",
                    "mandatory": false,
                    "defaultValue": "3.0d",
                    "readOnly": false,
                    "type": "double"
                },
                {
                    "name": "label-position-offset-left",
                    "label": "Label Position Offset Left",
                    "description": "The left offset of the label",
                    "mandatory": false,
                    "defaultValue": "3.0d",
                    "readOnly": false,
                    "type": "double"
                },
                {
                    "name": "label-position-offset-bottom",
                    "label": "Label Position Offset Bottom",
                    "description": "The bottom offset of the label",
                    "mandatory": false,
                    "defaultValue": "3.0d",
                    "readOnly": false,
                    "type": "double"
                },
                {
                    "name": "label-position-offset-right",
                    "label": "Label Position Offset Right",
                    "description": "The right offset of the label",
                    "mandatory": false,
                    "defaultValue": "3.0d",
                    "readOnly": false,
                    "type": "double"
                },
                {
                    "name": "color",
                    "label": "Color",
                    "description": "The marker color",
                    "mandatory": false,
                    "defaultValue": "",
                    "readOnly": false,
                    "type": "color"
                },
                {
                    "name": "alpha",
                    "label": "Alpha",
                    "description": "The marker transparency, must be between 0 and 1",
                    "mandatory": false,
                    "defaultValue": "",
                    "readOnly": false,
                    "type": "float",
                    "max": "1",
                    "min": "0"
                },
                {
                    "name": "stroke-width",
                    "label": "Stroke Width",
                    "description": "The width of the stroke",
                    "mandatory": false,
                    "defaultValue": "0",
                    "readOnly": false,
                    "type": "float",
                    "min": "0"
                },
                {
                    "name": "stroke-style",
                    "label": "Stroke Style",
                    "description": "The width of the stroke",
                    "mandatory": false,
                    "defaultValue": "",
                    "readOnly": false,
                    "type": "selectable_combo",
                    "comboOptions": [["STROKE_SOLID","Solid"],["STROKE_DOTTED", "Dotted"],["STROKE_DASHED","Dashed"]]
                },
                {
                    "name": "fontName",
                    "label": "Font Name",
                    "description": "The font used",
                    "mandatory": false,
                    "defaultValue": "SansSerif",
                    "readOnly": false,
                    "type": "fontfamily_combo"
