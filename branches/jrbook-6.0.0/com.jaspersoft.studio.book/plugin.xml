<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.jaspersoft.studio.book.editors.JRBookEditor"
            contributorClass="com.jaspersoft.studio.book.editors.JRBookEditorActionBarContributor"
            extensions="jrxml"
            default="false"
            icon="icons/sample.gif"
            id="com.jaspersoft.studio.book.editors.JRBookEditor"
            name="JasperReports Book Editor">
            <contentTypeBinding
                  contentTypeId="com.jaspersoft.studio.content-types.jrbook">
            </contentTypeBinding>
      </editor>
   </extension>
    <extension
          point="org.eclipse.core.contenttype.contentTypes">
       <content-type
             describer="com.jaspersoft.studio.book.editors.JRBookDescriber"
             file-extensions="jrxml"
             id="com.jaspersoft.studio.content-types.jrbook"
             name="JasperReports Book File"
             priority="low">
       </content-type>
    </extension>
    <extension
          point="com.jaspersoft.studio.templateProviderSupport">
       <templateProvider
             id="com.jaspersoft.studio.book.bundle.BookTemplateProvider"
             providerClass="com.jaspersoft.studio.book.bundle.BookTemplateProvider">
       </templateProvider>
    </extension>
    <extension
          point="com.jaspersoft.studio.components">
       <component
             ClassFactory="com.jaspersoft.studio.book.models.PartComponentFactory">
       </component>
    </extension>
    <extension
          point="com.jaspersoft.studio.properties.propertySections">
       <propertySections
             contributorId="com.jaspersoft.studio.editor.report.ReportContainer">
          <propertySection
                class="com.jaspersoft.studio.book.sections.ReportPartSection"
                id="com.jaspersoft.studio.book.sections.bookPart"
                tab="com.jaspersoft.studio.book.bookpart">
             <input
                   type="com.jaspersoft.studio.book.models.MReportPart">
             </input>
          </propertySection>
       </propertySections>
    </extension>
    <extension
          point="com.jaspersoft.studio.properties.propertyTabs">
       <propertyTabs
             contributorId="com.jaspersoft.studio.editor.report.ReportContainer">
          <propertyTab
                afterTab="com.jaspersoft.studio.editor.JrxmlEditor.Element"
                category="element"
                id="com.jaspersoft.studio.book.bookpart"
                label="Book Part"
                scrollable="true">
          </propertyTab>
       </propertyTabs>
    </extension>
</plugin>
