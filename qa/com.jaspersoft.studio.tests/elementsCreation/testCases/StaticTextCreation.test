get-view "Project Explorer" | get-tree | select MyReports | get-menu "New/Jasper Report" | click
with [get-window "New Report Wizard"] {
    get-button "Next >" | click
    get-editbox -after [get-label "File name:"] | set-text "StaticTextCreation.jrxml"
    get-button "Next >" | click
    get-combo -after [get-label "Data Adapter"] | select "One Empty Record - Empty rows"
    get-button "Next >" | click
    get-button Finish | click
}
with [get-view Palette | get-palette] {
    get-palette-entry Sort | mouse-move 70 0 -height 27 -width 575
    get-palette-entry "Page X of Y" | mouse-move 9 19 -height 27 -width 575
    with [get-palette-entry "Static Text"] {
        mouse-move 29 9 button1 -height 27 -width 575
        mouse-press 29 9 button1 -height 27 -width 575
        mouse-release 29 9 button1 524288 -height 27 -width 575
        mouse-move 9 9 -height 27 -width 575
    }
}
with [get-editor "StaticTextCreation.jrxml"] {
    with [get-diagram] {
        with [get-edit-part -className ReportPageEditPart | get-edit-part -className BandEditPart -index 3] {
            mouse-move 37 16 button1 -height 126 -width 596
            mouse-press 37 16 button1 -height 126 -width 596
            mouse-drag 52 34 -mask 524288 -height 126 -width 596
        }
        mouse-drag 68 245 -mask 524288 -height 620 -width 813
        mouse-drag 575 314 button1 524288 -height 620 -width 813
        mouse-release 575 314 button1 524288 -height 620 -width 813
